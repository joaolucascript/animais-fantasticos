{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": "off",
    "no-param-reassign": "off",
    "import/extensions": "off",
    "no-console": "off",
    "max-len": "off"
  }
}
